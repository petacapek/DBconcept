def Submodel (y, t, pars):
    #define initial states
    G=y[0];    Br=y[1];    Bs=y[2];     CO2=y[3]

    #define parameters
    v=pars[0];   k=pars[1];     f=pars[2]
    m=pars[3];   Y=pars[4];

    #Define fluxes
    ##glucose uptake
    uptake=v*Bs*G/(k+G)
    ##transfer function
    transfer=f*Br
    #death rate
    death=m*Bs

    #Define derivatives
    dGdt=-uptake
    dBrdt=uptake-transfer
    dBsdt=transfer*Y-death
    dCO2dt=transfer*(1-Y)

    return dGdt, dBrdt, dBsdt, dCO2dt;
