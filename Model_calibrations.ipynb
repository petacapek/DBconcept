    #define parameters
    pars = x

    #initial conditions
    G_i = d.Sinit[0]
    B_i = d.DNAinit[0]/pars[5]
    y0 = np.array([G_i, B_i, 0])

    #times
    t = d.Time

    #model simulations
    yhat_full = monodcalc(Monod, pars, t, y0)

